  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@biomejs/biome": "1.8.3",
    "tsup": "^8.2.4",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
